<h1 class="text-2xl font-bold my-5"><%= @team.name %>'s Daily Pair Dance</h1>

<.live_component
  id="pairing-table-wrapper"
  team={@team}
  module={PairDanceWeb.AppLive.TeamPage.PairingTableComponent}
/>

<.live_component
  id={1}
  module={PairDanceWeb.AppLive.TeamPage.CreateTaskComponent}
  team={@team}
  action={:new}
/>

<.live_component
  id="add-member-form"
  module={PairDanceWeb.AppLive.TeamMembersPage.AddMemberForm}
  team={@team}
/>
