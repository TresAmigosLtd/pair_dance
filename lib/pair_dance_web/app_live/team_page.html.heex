<h1 class="text-2xl mb-5"><%= @team.name %>'s Pair Dance 💃</h1>

<.live_component
  id="pairing-table-wrapper"
  team={@team}
  module={PairDanceWeb.AppLive.TeamPage.PairingTableComponent}
/>

<.live_component
  id={1}
  module={PairDanceWeb.AppLive.TeamPage.CreateTaskComponent}
  team={@team}
  action={:new}
/>

<h2 class="text-xl my-5">Settings ⚙️</h2>

<.live_component
  id="add-member-form"
  module={PairDanceWeb.AppLive.Settings.AddMemberForm}
  team={@team}
/>
